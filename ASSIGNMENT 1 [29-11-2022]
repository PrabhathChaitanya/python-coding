a=int(input("enter the 1st number:"))
b=int(input("enter the 2nd number:"))
c=int(input("enter the 3rd number:"))
d=int(input("enter the 4th number:"))
if a>b:
    if a>c:
        if a>d:
            print("a is big")
elif b>c:
    if b>d:
        print("b is big")
elif c>d:
    print("c is big")
else:
    print("d is big")
